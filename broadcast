val jobEndTime = new Timestamp(System.currentTimeMillis())

val updateQuery = s"""
UPDATE U_D_DSV_001_RIS_1.Ramp_Control_BAU_Jobs_Info
SET job_run_end_time = ?
WHERE Job_Id = ?
"""

val updateStmt = pconn.prepareStatement(updateQuery)
updateStmt.setTimestamp(1, jobEndTime)
updateStmt.setString(2, jobId)
updateStmt.executeUpdate()

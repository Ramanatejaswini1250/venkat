val minSize = math.min(MasterTable2DF.size, Master2CSVrows.size)

for (i <- 0 until minSize) {  
    if (MasterTable2DF(i) != Master2CSVrows(i)) {  
        println(s"Mismatch found at row $i: Table: ${MasterTable2DF(i)} != CSV: ${Master2CSVrows(i)}")  
        dataConsistent2 = false  
    }  
}  

// Handle extra rows if sizes are different
if (MasterTable2DF.size != Master2CSVrows.size) {
    println(s"Warning: Size mismatch - MasterTable2DF has ${MasterTable2DF.size} rows, but Master2CSVrows has ${Master2CSVrows.size} rows.")
    dataConsistent2 = false
}
